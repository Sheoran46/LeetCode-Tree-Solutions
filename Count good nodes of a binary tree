/**
 * Definition for a binary tree node.
 * public class TreeNode {
 *     int val;
 *     TreeNode left;
 *     TreeNode right;
 *     TreeNode() {}
 *     TreeNode(int val) { this.val = val; }
 *     TreeNode(int val, TreeNode left, TreeNode right) {
 *         this.val = val;
 *         this.left = left;
 *         this.right = right;
 *     }
 * }
 */
class Solution {
    int result=0;
    public void find(TreeNode root,  int leftmax){
        if(root==null)
        return;
        if(root.val>=leftmax){
            result++;
        }
        find(root.left, Math.max(leftmax, root.val));
        find(root.right, Math.max(leftmax, root.val));
    }
    public int goodNodes(TreeNode root) {
        find(root, Integer.MIN_VALUE);
        return result;
    }
}
